 <div class="container">
 	<div class="row">
 		<div class="col-md-4 opyellow" style="padding-bottom: 80 px; color: black; border: 2px solid; border-radius: 25px;">
 			<h2>Log in</h2>

 			<%= form_for(@user, url: session_path(resource_name)) do |f| %>
 			<%= f.email_field :email %>
 			<%= f.password_field :password %>
 			</br>
 			<%= f.check_box :remember_me %>
 			<%= f.submit "Log In" %>
 			<% end %> 


 			<%= render "devise/shared/links" %>


 		</div>
 	</div>
 </div>
