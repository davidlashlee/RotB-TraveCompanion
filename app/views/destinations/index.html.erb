

 <div class="container">
  <div class="row">
    <div class="col-md-6">
    <h3>Listing Destinations</h1>
<table class="table table-striped table-hover ">
  <thead>

    <tr>
      <th>Location</th>
      <th>Start Date</th>
      <th>End Date</th>
   <!--    <th>User ID</th> -->
    </tr>
  </thead>
  <tbody>
    <% @destinations.each do |destination| %>
    <tr class="success">
      <td><%= destination.location %></td>
      <td><%= destination.start_date %> </td>
      <td><%= destination.end_date %> </td>
    <!--   <td><%= destination.user_id %> </td> -->
      <td><%= link_to 'Show', destination_path(destination) %></td>
      <td><%= link_to 'Edit', edit_destination_path(destination) %></td>
      <td><%= link_to 'Destroy', destination_path(destination), method: :delete, data: { confirm: 'Sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table> 
</div>
</div>
</div>

