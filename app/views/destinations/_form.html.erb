<%= form_for @destination do |f| %>

  <p>
    <%= f.label :Location %><br>
    <%= f.text_field :location %>
  </p>
 
  <p>
    <%= f.label :Start_date %><br>
    <%= f.text_field :start_date %>
  </p>
 
 <p>
    <%= f.label :End_date %><br>
    <%= f.text_field :end_date %>
  </p>

  <p>
    <%= f.label :User_id %><br>
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>