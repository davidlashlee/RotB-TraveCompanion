
<div class="container">
  <div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-8">
<h3>Welcome</h3>


<h6>Users: <%= User.count %> registered</h6>

<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Email</th>
      <th>Gender</th>
      <th>Age</th>
    </tr>
  </thead>
  <tbody>
    <% User.all.each do |user| %>
    <tr class="success">
          <td><%= user.email %></td>
          <td><%= user.gender %> </td>
          <td><%= user.age %> </td>
    </tr>
  <% end %>
  </tbody>
</table> 
</div>
</div>
</div>